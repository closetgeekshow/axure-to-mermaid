{
  "dependencies": {
    "js-base64": "^3.7.7",
    "pako": "^2.1.0"
  },
  "name": "axure-to-mermaid",
  "version": "1.0.0",
  "description": "This script is designed to be used with [Axure RP](https://axure.com) (a phenomenal wireframing tool) prototypes, when it is run it will copy your published sitemap tree as a mermaid flowchart to your clipboard.",
  "main": "axure-to-mermaid.js",
  "devDependencies": {
    "esbuild": "0.24.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "esbuild axure-to-mermaid.js --bundle --outfile=./dist/axure-to-mermaid.js",
    "build:esm": "esbuild axure-to-mermaid.js --bundle --format=esm --tree-shaking --outfile=./dist/axure-to-mermaid.esm.js",
    "build:min": "esbuild axure-to-mermaid.js --bundle --minify --format=esm --tree-shaking --outfile=./dist/axure-to-mermaid.esm.min.js",
    "build:iife": "esbuild axure-to-mermaid-injected.js --bundle --format=iife --minify --outfile=./dist/axure-to-mermaid.iife.min.js",
    "build": "npm run build:dev && npm run build:esm && npm run build:min && npm run build:iife && npm run size-report",
    "size-report": "ls -lh ./dist | awk 'BEGIN {print \"Size\\tFile\\n----\\t----\"} NR>1 {print $5\"\\t\"$9}' | column -t"
  
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
